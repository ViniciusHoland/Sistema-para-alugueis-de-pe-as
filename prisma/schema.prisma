

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int  @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String   @unique
}

model Peca {

  id Int @id @default(autoincrement())
  name String
  valorDiario Decimal @db.Decimal(10,2)

}

model Cliente {
  id Int @id @default(autoincrement())
  nome String
  email String @unique
  telefone String
  endereco Json
}


